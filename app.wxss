/**app.wxss**/

@import "./assets/iconfont/iconfont.wxss";


.container {
  padding: 0;
  height: 100vh;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  color: #333;
  background-color: #f5fafe;
  font-size: 30rpx;
}

.copyright {
  align-self: center;
  flex: 1;
  display: flex;
  align-items: flex-end;
  padding-bottom: 20rpx;
  font-size: 28rpx;
  color: #999;
}

.view-hover {
  background-color: #f3f3f3!important;
}